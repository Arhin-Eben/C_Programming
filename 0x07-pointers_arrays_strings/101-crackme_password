#include <stdio.h>
#include <string.h>

/**
 * main - Entry point of the program.
 *
 * Return: 0 on success.
 */
int main(void)
{
	FILE *file;
	const char *password = "ltrace ldd gdb objdump";
	const char *filename = "password.txt";

	file = fopen(filename, "w");
	if (file == NULL)
	{
		printf("Failed to create the file.n");
		return 1;
	}

	fprintf(file, "%s", password);
	fclose(file);

	printf("Password file created successfully.n");

	return (0);
}
